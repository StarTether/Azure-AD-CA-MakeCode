{
  "id": "camakecode",
  "name": "CaMakeCode",
  "title": "MakeCode Editor for Azure AD Conditional Access Policies",
  "corepkg": "core",
  "cloud": {
    "workspace": false,
    "packages": true
  },
  "runtime": {
    "mathBlocks": false,
    "loopsBlocks": true,
    "logicBlocks": true,
    "variablesBlocks": false,
    "textBlocks": false,
    "functionBlocks": false,
    "listsBlocks": false
  },
  "simulator": {
    "autoRun": true
  },
  "versions": {
    "target": "2.0.2",
    "pxt": "5.6.4"
  },
  "blocksprj": {
    "id": "blocksprj",
    "config": {
      "name": "{0}",
      "dependencies": {
        "core": "*"
      },
      "description": "",
      "files": [
        "main.blocks",
        "main.ts",
        "README.md"
      ],
      "additionalFilePaths": []
    },
    "files": {
      "main.blocks": "<xml xmlns=\"http://www.w3.org/1999/xhtml\">\n</xml>",
      "main.ts": " ",
      "README.md": " "
    }
  },
  "tsprj": {
    "id": "tsprj",
    "config": {
      "name": "{0}",
      "dependencies": {
        "core": "*"
      },
      "description": "",
      "files": [
        "main.ts",
        "README.md"
      ],
      "additionalFilePaths": []
    },
    "files": {
      "main.ts": " ",
      "README.md": " "
    }
  },
  "compile": {
    "isNative": false,
    "hasHex": false,
    "switches": {},
    "jsRefCounting": false,
    "vtableShift": 2,
    "noSourceInFlash": true,
    "utf8": true
  }
}